pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Shilpakcap/TestJenkin.git'
            }
        }
        stage('Debug Workspace') {
            steps {
                script {
                    bat 'echo Current Directory: %CD%'
                    bat 'dir /s /b'
                }
            }
        }

        stage('List Files') {
            steps {
                script {
                    bat 'dir /s /b'  // Lists all files and folders for debugging
                }
            }
        }
        stage('Run Jenkinsfile') {
            steps {
                script {
                    dir('scripts') {  // Navigate to the correct subdirectory
                        bat 'python E_Jenkinsfile'  // Execute script
                    }
                }
            }
        }
    }
}
