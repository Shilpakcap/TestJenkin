pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Shilpakcap/TestJenkin.git'
            }
        }
        stage('Run E_Jenkinsfile') {
            steps {
                script {
                    // Change to the 'Scripts' directory
                    dir('Scripts') {
                        // Run the script directly if it's a batch file or shell script
                        bat 'E_Jenkinsfile'  // Assuming it's a batch script
                    }
                }
            }
        }
    }
}
