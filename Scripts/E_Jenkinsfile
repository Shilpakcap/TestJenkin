pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Shilpakcap/TestJenkin.git'
            }
        }
        stage('Verify File Exists') {
            steps {
                script {
                    bat 'dir /s /b'  // List all files to verify Jenkins sees them
                    bat 'type Scripts\\E_Jenkinsfile'  // Correct path to the file
                }
            }
        }
    }
}
