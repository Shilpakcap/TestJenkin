pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Shilpakcap/TestJenkin.git'
            }
        }
        stage('Run E_Jenkinsfile') {
            steps {
                script {
                    // Change to the 'Scripts' directory
                    dir('Scripts') {
                        // Read and run the Jenkinsfile
                        load 'E_Jenkinsfile'  // Load the Jenkinsfile for execution
                    }
                }
            }
        }
    }
}
